package public

import "github.com/madalinpopa/gocost-web/ui/templates/layouts"
import "github.com/madalinpopa/gocost-web/internal/interfaces/web"

templ Index(data web.Data) {
	@layouts.Main(data) {
		<div class="h-full selection:bg-primary-500 selection:text-white flex flex-col relative">
			@Hero(data)
			@Features()
			@Workflow()
		</div>
	}
}
